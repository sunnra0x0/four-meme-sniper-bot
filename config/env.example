# Four.Meme Sniper Bot Configuration

# Blockchain Configuration
BSC_RPC_URL=https://bsc-dataseed.binance.org/
BSC_TESTNET_RPC_URL=https://data-seed-prebsc-1-s1.binance.org:8545/
PRIVATE_KEY=your_private_key_here
WALLET_ADDRESS=your_wallet_address_here

# Database Configuration
MONGODB_URI=mongodb://localhost:27017/four-meme-sniper
REDIS_URL=redis://localhost:6379

# Four.Meme API Configuration
FOUR_MEME_API_URL=https://api.four.meme
FOUR_MEME_API_KEY=your_four_meme_api_key_here

# Bot Configuration
MAX_SLIPPAGE=0.05
MAX_GAS_PRICE=20
MIN_LIQUIDITY=10000
MAX_POSITION_SIZE=0.1
PROFIT_THRESHOLD=0.01
STOP_LOSS_PERCENTAGE=10
TAKE_PROFIT_PERCENTAGE=20

# Four.Meme Specific Configuration
BONDING_CURVE_THRESHOLD=0.8
FAIR_LAUNCH_DELAY=1000
FOUR_MEME_TOKEN_PATTERN=four\.meme|fourmeme
MIN_BONDING_CURVE_PROGRESS=0.1
MAX_BONDING_CURVE_PROGRESS=0.9

# Risk Management
MAX_RISK_SCORE=0.8
MAX_DAILY_LOSS=0.5
MAX_CONCURRENT_TRADES=5
POSITION_SIZE_MULTIPLIER=1.0

# Monitoring Configuration
MONITORING_INTERVAL=5000
PRICE_ALERT_THRESHOLD=0.1
LIQUIDITY_ALERT_THRESHOLD=1000
TOKEN_DETECTION_INTERVAL=2000
BONDING_CURVE_MONITORING_INTERVAL=2000

# DEX Configuration
DEFAULT_DEX=PancakeSwap
ENABLED_DEXES=PancakeSwap,PancakeSwapV3,UniswapV2

# PancakeSwap Configuration
PANCAKE_ROUTER_ADDRESS=0x10ED43C718714eb63d5aA57B78B54704E256024E
PANCAKE_FACTORY_ADDRESS=0xcA143Ce0Fe65960E6Aa4D42C8D3cE161c2B6594
PANCAKE_WBNB_ADDRESS=0xbb4CdB9CBd36B01bD1cBaEBF2De08d9173bc095c

# PancakeSwap V3 Configuration
PANCAKE_V3_ROUTER_ADDRESS=0x1b81D678ffb9C0263b24A97847620C99d213eB14
PANCAKE_V3_FACTORY_ADDRESS=0x0BFbCF9fa4f9C56B0F40a671Ad40E0805A091865

# MEV Protection
ENABLE_MEV_PROTECTION=true
MEV_PROTECTION_DELAY=100
PRIVATE_MEMPOOL=true

# Gas Optimization
GAS_PRICE_BUFFER=1.1
GAS_LIMIT_BUFFER=1.2
DYNAMIC_GAS_PRICING=true

# Logging Configuration
LOG_LEVEL=info
LOG_FILE=./logs/four-meme-sniper.log
LOG_MAX_SIZE=10m
LOG_MAX_FILES=5

# API Configuration
API_PORT=3001
API_HOST=localhost
CORS_ORIGIN=http://localhost:3000

# WebSocket Configuration
WS_PORT=3002
WS_HOST=localhost

# Notification Configuration
ENABLE_TELEGRAM_NOTIFICATIONS=false
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
TELEGRAM_CHAT_ID=your_telegram_chat_id

ENABLE_DISCORD_NOTIFICATIONS=false
DISCORD_WEBHOOK_URL=your_discord_webhook_url

# Security Configuration
ENABLE_RATE_LIMITING=true
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# Performance Configuration
ENABLE_CACHING=true
CACHE_TTL=300
MAX_CACHE_SIZE=1000

# Monitoring and Metrics
ENABLE_METRICS=true
METRICS_PORT=9090
HEALTH_CHECK_INTERVAL=30000

# Development Configuration
NODE_ENV=development
DEBUG=false
ENABLE_HOT_RELOAD=false